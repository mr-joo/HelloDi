<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- 홍길동 -->
	<bean id="hgd" class="kr.co.oraclejava.di.Person">
		<property name="id">
			<value>hgd</value>
		</property>

		<property name="name">
			<value>홍길동</value>
		</property>

		<property name="age">
			<value>16</value>
		</property>
	</bean>
	
	<!-- 가나다 -->
	<bean id="gnd" class="kr.co.oraclejava.di.Person">
		<property name="id">
			<value>gnd</value>
		</property>

		<property name="name">
			<value>가나다</value>
		</property>

		<property name="age">
			<value>16</value>
		</property>
	</bean>
</beans>
